<models>
    <model name="rabbit" showcontained="false" usescaleof="height">
        <submodels>
            <submodel mesh="3d_objects/creatures/rabbit/RabbitHi.mesh">
                <parts>
                    <part name="main" show="true" />
                </parts>
            </submodel>
        </submodels>
        <actions>
            <action name="__movement_walk">
                <activations>
                    <activation type="movement">walk</activation>
                </activations>
                <animations>
                    <animation iterations="1">
                        <animationpart name="Run" weight="1" />
                    </animation>
                    <animation iterations="2">
                        <animationpart name="Run2" weight="1.2" />
                    </animation>
                </animations>
            </action>
            <action name="__movement_idle" speed="1.000000">
                <activations>
                    <activation type="movement">idle</activation>
                </activations>
                <animations>
                    <animation iterations="1">
                        <animationpart name="SitUp" weight="1" />
                    </animation>
                    <animation iterations="2">
                        <animationpart name="LookAround" weight="1" />
                    </animation>
                    <animation iterations="4">
                        <animationpart name="Eat" weight="1" />
                    </animation>
                    <animation iterations="1">
                        <animationpart name="LookAround" weight="1" />
                    </animation>
                    <animation iterations="2">
                        <animationpart name="Scratch" weight="1" />
                    </animation>
                    <animation iterations="2">
                        <animationpart name="Eat2" weight="1" />
                    </animation>
                    <animation iterations="3">
                        <animationpart name="LookAround" weight="1" />
                    </animation>
                    <animation iterations="1">
                        <animationpart name="SitUp" weight="1" />
                    </animation>
                </animations>
            </action>
            <action name="drink" speed="1.000000">
                <activations>
                    <activation type="action">drink</activation>
                </activations>
                <animations>
                    <animation iterations="1">
                        <animationpart name="Eat2" weight="1.000000" />
                    </animation>
                </animations>
            </action>
            <action name="sit" speed="1.000000">
                <activations>
                    <activation type="action">sit</activation>
                </activations>
                <animations>
                    <animation iterations="1">
                        <animationpart name="Sit" weight="1.000000" />
                    </animation>
                </animations>
            </action>
        </actions>
        <attachpoints />
        <views />
        <lights />
    </model>
</models>